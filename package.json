    "koajax": "^0.8.3",
    "lodash.debounce": "^4.0.8",
    "mobx": "^4.15.7",
    "mobx-i18n": "^0.3.14",
    "mobx-react": "^6.3.1",
    "mobx-restful": "^0.6.2",
    "preact": "^10.12.1",
    "query-string": "^8.1.0",
    "web-utility": "^4.0.0"
